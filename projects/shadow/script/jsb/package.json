{
    "scripts": {
        "gfx-main": "tsc -p ../../../../script/gfx/main --outDir dist/gfx-main --watch",
        "engine-main": "tsc -p ../../../../script/engine/main --outDir dist/engine-main --watch",
        "engine-jsb": "tsc -p ../../../../script/engine/jsb --outDir dist/engine-jsb --watch",
        "main": "tsc -p ../main --outDir dist/main --watch",
        "dev": "concurrently \"tsc --watch\" \"npm run gfx-main\" \"npm run engine-main\" \"npm run engine-jsb\" \"npm run main\""
    },
    "dependencies": {
        "engine-jsb": "file:../../../../script/engine/jsb",
        "main": "file:../main"
    }
}