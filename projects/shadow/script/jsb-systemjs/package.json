{
    "type": "module",
    "scripts": {
        "yaml": "rollup -c -i ../../../../script/engine/main/node_modules/yaml/browser/index.js -o ./dist/yaml.js",
        "spine-core": "rollup -c -i ../../../../script/engine/main/node_modules/@esotericsoftware/spine-core/dist/index.js -o ./dist/spine-core.js",
        "gfx-main": "tsc -p ../../../../script/gfx/main --outDir ./dist/gfx-main --module system --watch",
        "engine-main": "tsc -p ../../../../script/engine/main --outDir ./dist/engine-main --module system --watch",
        "engine-jsb": "tsc -p ../../../../script/engine/jsb --outDir ./dist/engine-jsb --module system --watch",
        "main": "tsc -p ../main --outDir ./dist/main --module system --watch",
        "dev": "concurrently \"tsc --watch\" \"npm run gfx-main\" \"npm run engine-main\" \"npm run engine-jsb\" \"npm run main\""
    },
    "dependencies": {
        "@rollup/plugin-babel": "^6.0.3",
        "engine-jsb": "file:../../../../script/engine/jsb"
    }
}