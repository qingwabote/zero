{
    "type": "module",
    "scripts": {
        "yaml": "rollup -i ../../../../script/engine/main/node_modules/yaml/browser/index.js -o dist/yaml.js",
        "spine-core": "rollup -i ../../../../script/engine/main/node_modules/@esotericsoftware/spine-core/dist/index.js -o dist/spine-core.js",
        "gfx-common": "tsc -p ../../../../script/gfx/common --outDir dist/gfx-common --watch",
        "gfx": "tsc -p ../../../../script/gfx/webgl2 --outDir dist/gfx --watch",
        "loader": "tsc -p ../../../../script/loader/web --outDir dist/loader --watch",
        "phys": "tsc -p ../../../../script/phys/ammo --outDir dist/phys --watch",
        "engine-main": "tsc -p ../../../../script/engine/main --outDir dist/engine-main --watch",
        "engine-web": "tsc -p ../../../../script/engine/web --outDir dist/engine-web --watch",
        "main": "tsc -p ../main --outDir ./dist/main --watch",
        "server": "http-server ../../../../ -p 8003 -o projects/skin/script/web/index.html -e js -c-1",
        "dev": "concurrently \"tsc --watch\" \"npm run gfx-common\" \"npm run gfx\" \"npm run loader\" \"npm run phys\" \"npm run engine-main\" \"npm run engine-web\" \"npm run main\" \"npm run server\""
    },
    "dependencies": {
        "engine-web": "file:../../../../script/engine/web",
        "main": "file:../main"
    }
}